          <field field_ref="sopstadv_pac"    name="sopstadv_pacific"   standard_name="advective_salt_transport_pacific" />
          <field field_ref="sopstadv_ind"    name="sopstadv_indian"   standard_name="advective_salt_transport_indian"   />
          <field field_ref="sopstadv_ipc"    name="sopstadv_ipc"   standard_name="advective_salt_transport_indopacific"  />
          <field field_ref="sopstbtr"     name="sltovbaro"   standard_name="barotropic_salt_transport"     />
          <field field_ref="sopstbtr_atl"    name="sltovbaro_atlantic"   standard_name="barotropic_salt_transport_atlantic"/>
          <field field_ref="sopstbtr_pac"    name="sltovbaro_pacific"   standard_name="barotropic_salt_transport_pacific" />
          <field field_ref="sopstbtr_ind"    name="sltovbaro_indian"   standard_name="barotropic_salt_transport_indian"   />
          <field field_ref="sopstbtr_ipc"    name="sltovbaro_ipc"   standard_name="barotropic_salt_transport_indopacific"  />
          <!--The following variables are to be merged into a single variableis with a basin dimenesion in postprocessing  -->
          <!-- hfbasin -->
          <field field_ref="sophtadv" name="hfbasin_global" standard_name="northward_ocean_heat_transport" > @sophtadv + @sophtldf </field>
          <field field_ref="sophtadv" name="hfbasin_atlantic" standard_name="northward_ocean_heat_transport" > @sophtadv_atl + @sophtldf_atl </field>
          <field field_ref="sophtadv" name="hfbasin_pacific" standard_name="northward_ocean_heat_transport" > @sophtadv_pac + @sophtldf_pac </field>
          <field field_ref="sophtadv" name="hfbasin_indian" standard_name="northward_ocean_heat_transport" > @sophtadv_ind + @sophtldf_ind </field>
          <field field_ref="sophtadv" name="hfbasin_indopacific" standard_name="northward_ocean_heat_transport" > @sophtadv_ipc + @sophtldf_ipc </field>
          <!-- hfbasinpadv -->
          <field field_ref="sophteiv"     name="hfbasinpadv_global"   standard_name="heat_transport_due_to_parameterized_eddy_advection_global"                       grid_ref="gznl_T_2D"       />
          <field field_ref="sophteiv_atl"    name="hfbasinpadv_atlantic"   standard_name="heat_transport_due_to_parameterized_eddy_advection_atlantic"/>
          <field field_ref="sophteiv_pac"    name="hfbasinpadv_pacific"   standard_name="heat_transport_due_to_parameterized_eddy_advection_pacific" />
          <field field_ref="sophteiv_ind"    name="hfbasinpadv_indian"   standard_name="heat_transport_due_to_parameterized_eddy_advection_indian"   />
          <field field_ref="sophteiv_ipc"    name="hfbasinpadv_indopacific"   standard_name="heat_transport_due_to_parameterized_eddy_advection_indopacific"  />
          <!-- hfbasinpmadv -->
          <field field_ref="sophteiv"     name="hfbasinpmadv_global"   standard_name="heat_transport_due_to_parameterized_mesoscale_eddy_advection_global"                       grid_ref="gznl_T_2D"       />
          <field field_ref="sophteiv_atl"    name="hfbasinpmadv_atlantic"   standard_name="heat_transport_due_to_parameterized_mesoscale_eddy_advection_atlantic" />
          <field field_ref="sophteiv_pac"    name="hfbasinpmadv_pacific"   standard_name="heat_transport_due_to_parameterized_mesoscale_eddy_advection_pacific" />
          <field field_ref="sophteiv_ind"    name="hfbasinpmadv_indian"   standard_name="heat_transport_due_to_parameterized_mesoscale_eddy_advection_indian"   />
          <field field_ref="sophteiv_ipc"    name="hfbasinpmadv_indopacific"   standard_name="heat_transport_due_to_parameterized_mesoscale_eddy_advection_indopacific"   />
          <!-- hfbasinpmdiff -->
          <field field_ref="sophtldf"     name="hfbasinpmdiff_global"   standard_name="northward_ocean_heat_transport_due_to_parameterized_mesoscale_diffusion_global"                       grid_ref="gznl_T_2D"       />
          <field field_ref="sophtldf_atl"    name="hfbasinpmdiff_atlantic"   standard_name="northward_ocean_heat_transport_due_to_parameterized_mesoscale_diffusion_atlantic"/>
          <field field_ref="sophtldf_pac"    name="hfbasinpmdiff_pacific"   standard_name="northward_ocean_heat_transport_due_to_parameterized_mesoscale_diffusion_pacific" />
          <field field_ref="sophtldf_ind"    name="hfbasinpmdiff_indian"   standard_name="northward_ocean_heat_transport_due_to_parameterized_mesoscale_diffusion_indian"  />
          <field field_ref="sophtldf_ipc"    name="hfbasinpmdiff_indopacific"   standard_name="northward_ocean_heat_transport_due_to_parameterized_mesoscale_diffusion_indopacific"   />
          <!-- hfovovrt -->
          <field field_ref="sophtove"     name="hfovovrt_global"   standard_name="overturning_heat_transport_global"                   grid_ref="gznl_T_2D"       />
          <field field_ref="sophtove_atl"    name="hfovovrt_atlantic"   standard_name="overturning_heat_transport_atlantic"/>
          <field field_ref="sophtove_pac"    name="hfovovrt_pacific"   standard_name="overturning_heat_transport_pacific" />
          <field field_ref="sophtove_ind"    name="hfovovrt_indian"   standard_name="overturning_heat_transport_indian"   />
          <field field_ref="sophtove_ipc"    name="hfovovrt_indopacific"   standard_name="overturning_heat_transport_indopacific"   />
          <!-- hfovgyre Note that the barotropic term is NOT subtracted, because the CMIP6 data request specifies that it shouldn't be.-->
          <field field_ref="sophtove"     name="hfovgyre_global"   standard_name="gyre_heat_transport_global"         > @sophtadv - @sophtove </field>
          <field field_ref="sophtove_atl"    name="hfovgyre_atlantic"   standard_name="gyre_heat_transport_atlantic" > @sophtadv_atl - @sophtove_atl </field>
          <field field_ref="sophtove_pac"    name="hfovgyre_pacific"   standard_name="gyre_heat_transport_pacific"  > @sophtadv_pac - @sophtove_pac </field>
          <field field_ref="sophtove_ind"    name="hfovgyre_indian"   standard_name="gyre_heat_transport_indian"   > @sophtadv_ind - @sophtove_ind </field>
          <field field_ref="sophtove_ipc"    name="hfovgyre_indopacific"   standard_name="gyre_heat_transport_indopacific"   > @sophtadv_ipc - @sophtove_ipc </field>
          <!-- sltbasin -->
          <field field_ref="sopstadv" name="sltbasin_global" standard_name="northward_ocean_salt_transport_global" unit="kg s-1"> (@sopstadv + @sopstldf)/1000 </field>
          <field field_ref="sopstadv" name="sltbasin_atlantic" standard_name="northward_ocean_salt_transport_atlantic" unit="kg s-1"> (@sopstadv_atl + @sopstldf_atl)/1000 </field>
          <field field_ref="sopstadv" name="sltbasin_pacific" standard_name="northward_ocean_salt_transport_pacific" unit="kg s-1"> (@sopstadv_pac + @sopstldf_pac)/1000 </field>
          <field field_ref="sopstadv" name="sltbasin_indian" standard_name="northward_ocean_salt_transport_indian" unit="kg s-1"> (@sopstadv_ind + @sopstldf_ind)/1000 </field>
          <field field_ref="sopstadv" name="sltbasin_indopacific" standard_name="northward_ocean_salt_transport_indopacific" unit="kg s-1"> (@sopstadv_ipc + @sopstldf_ipc)/1000 </field>
          <!-- sltbasinpadv -->
          <field field_ref="sopsteiv"        name="sltbasinpadv_global"   standard_name="salt_transport_due_to_parameterized_eddy_advection_global"  unit="kg s-1"> @sopsteiv/1000   </field>
          <field field_ref="sopsteiv_atl"    name="sltbasinpadv_atlantic"   standard_name="salt_transport_due_to_parameterized_eddy_advection_atlantic" unit="kg s-1"> @sopsteiv_atl/1000   </field>
          <field field_ref="sopsteiv_pac"    name="sltbasinpadv_pacific"   standard_name="salt_transport_due_to_parameterized_eddy_advection_pacific" unit="kg s-1"> @sopsteiv_pac/1000   </field>
          <field field_ref="sopsteiv_ind"    name="sltbasinpadv_indian"   standard_name="salt_transport_due_to_parameterized_eddy_advection_indian"  unit="kg s-1"> @sopsteiv_ind/1000   </field>
          <field field_ref="sopsteiv_ipc"    name="sltbasinpadv_indopacific"   standard_name="salt_transport_due_to_parameterized_eddy_advection_indopacific"  unit="kg s-1"> @sopsteiv_ipc/1000   </field>
          <!-- sltbasinpmadv -->
          <field field_ref="sopsteiv"        name="sltbasinpmadv_global"   standard_name="salt_transport_due_to_parameterized_mesoscale_eddy_advection_global"  unit="kg s-1"> @sopsteiv/1000   </field>        
          <field field_ref="sopsteiv_atl"    name="sltbasinpmadv_atlantic"   standard_name="salt_transport_due_to_parameterized_mesoscale_eddy_advection_atlantic" unit="kg s-1"> @sopsteiv_atl/1000   </field>
          <field field_ref="sopsteiv_pac"    name="sltbasinpmadv_pacific"   standard_name="salt_transport_due_to_parameterized_mesoscale_eddy_advection_pacific" unit="kg s-1"> @sopsteiv_pac/1000   </field>
          <field field_ref="sopsteiv_ind"    name="sltbasinpmadv_indian"   standard_name="salt_transport_due_to_parameterized_mesoscale_eddy_advection_indian"  unit="kg s-1"> @sopsteiv_ind/1000   </field>
          <field field_ref="sopsteiv_ipc"    name="sltbasinpmadv_indopacific"   standard_name="salt_transport_due_to_parameterized_mesoscale_eddy_advection_indopacific"  unit="kg s-1"> @sopsteiv_ipc/1000   </field>
          <!-- sltbasinpmdiff -->
          <field field_ref="sopstldf"     name="sltbasinpmdiff_global"   standard_name="northward_ocean_salt_transport_due_to_parameterized_mesoscale_diffusion_global" unit="kg s-1">   @sopstldf/1000   </field>
          <field field_ref="sopstldf_atl"    name="sltbasinpmdiff_atlantic"   standard_name="northward_ocean_salt_transport_due_to_parameterized_mesoscale_diffusion_atlantic" unit="kg s-1"> @sopstldf_atl/1000   </field>
          <field field_ref="sopstldf_pac"    name="sltbasinpmdiff_pacific"   standard_name="northward_ocean_salt_transport_due_to_parameterized_mesoscale_diffusion_pacific" unit="kg s-1"> @sopstldf_pac/1000   </field>
          <field field_ref="sopstldf_ind"    name="sltbasinpmdiff_indian"   standard_name="northward_ocean_salt_transport_due_to_parameterized_mesoscale_diffusion_indian" unit="kg s-1">  @sopstldf_ind/1000   </field>
          <field field_ref="sopstldf_ipc"    name="sltbasinpmdiff_indopacific"   standard_name="northward_ocean_salt_transport_due_to_parameterized_mesoscale_diffusion_indopacific" unit="kg s-1">  @sopstldf_ipc/1000   </field>
          <!-- sltovovrt -->
          <field field_ref="sopstove"     name="sltovovrt_global"   standard_name="overturning_salt_transport_global"  unit="kg s-1">   @sopstove/1000   </field>       
          <field field_ref="sopstove_atl"    name="sltovovrt_atlantic"   standard_name="overturning_salt_transport_atlantic" unit="kg s-1">   @sopstove_atl/1000   </field>
          <field field_ref="sopstove_pac"    name="sltovovrt_pacific"   standard_name="overturning_salt_transport_pacific"  unit="kg s-1">   @sopstove_pac/1000   </field>
          <field field_ref="sopstove_ipc"    name="sltovovrt_indopacific"   standard_name="overturning_salt_transport_indopacific"   unit="kg s-1">   @sopstove_ipc/1000   </field>
          <!-- sltovgyre Note that the barotropic term is NOT subtracted, because the CMIP6 data request specifies that it shouldn't be.-->
          <field field_ref="sopstove"     name="sltovgyre_global"   standard_name="gyre_salt_transport_global"    unit="kg s-1" > (@sopstadv - @sopstove)/1000 </field>
          <field field_ref="sopstove_atl"    name="sltovgyre_atlantic"   standard_name="gyre_salt_transport_atlantic" unit="kg s-1" > (@sopstadv_atl - @sopstove_atl)/1000 </field>
          <field field_ref="sopstove_pac"    name="sltovgyre_pacific"   standard_name="gyre_salt_transport_pacific" unit="kg s-1"  > (@sopstadv_pac - @sopstove_pac)/1000 </field>
          <field field_ref="sopstove_ind"    name="sltovgyre_indian"   standard_name="gyre_salt_transport_indian" unit="kg s-1"   > (@sopstadv_ind - @sopstove_ind)/1000 </field>
          <field field_ref="sopstove_ipc"    name="sltovgyre_indopacific"   standard_name="gyre_salt_transport_indopacific" unit="kg s-1"   > (@sopstadv_ipc - @sopstove_ipc)/1000 </field>
        </file>

        <file id="file11" name_suffix="_ptrc_T" description="Medusa sms variables" >
          <field field_ref="e3t"          name="thkcello"        standard_name="cell_thickness" />
          <field_group group_ref="groupMEDUSA"      enabled=".TRUE."  />   <!-- key_medusa -->
          <field_group group_ref="groupIDTRA"       enabled=".TRUE."  />   <!-- key_medusa -->
